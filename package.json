{
  "name": "vue3-directive",
  "version": "0.1.0",
  "description": "Various instruction plug-in collection",
  "keywords": [
    "vue3",
    "directive",
    "vue3-directive",
    "click-outside",
    "scroll-lock",
    "clipboard",
    "watermarker",
    "resize-text",
    "permission",
    "long-click"
  ],
  "private": false,
  "main": "dist/vue3-directive.esm.js",
  "module": "dist/vue3-directive.esm.js",
  "types": "dist/packages/index.d.ts",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:es": "rollup --config build/rollup.config.js --format es && rimraf dist/src && yarn build:version",
    "build:version": "node build/gen-version.js",
    "clean:dist": "rimraf dist",
    "pub": "cd dist && npm publish --registry=http://registry.npmjs.org && cd ..",
    "unpub": "npm unpublish vue3-storage --force --registry=http://registry.npmjs.org",
    "deploy": "rimraf dist && yarn build && gh-pages -d dist"
  },
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "lodash.debounce": "^4.0.8",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0"
  },
  "peerDependencies": {
    "body-scroll-lock": "^3.1.5",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "lodash.debounce": "^4.0.8",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-auto-install": "^2.1.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/clipboard": "^2.0.1",
    "@types/fs-extra": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "fs-extra": "^9.1.0",
    "gh-pages": "^3.1.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3"
  }
}
